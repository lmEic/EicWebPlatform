<div class="panel panel-primary" ng-controller="developmentInputRecordCtrl">
        <div class="panel-heading">
        <h6 class="panel-title">设计开发输入审查表</h6>
    </div>
    <div class="DivInput">
        <div class="panel-body">
                <div bs-pane data-title="设计开发输入登记" data-name="initTab">
                    <div class="row">
                        <div class="col-md-8">
                            <div class="well">
                                <form role="form" name="formdevelopment" novalidate class="form-horizontal" onkeypress="if (event.keyCode === 13) return false;">
                                    <fieldset>
                                        <legend>发文单位信息</legend>
                                        <div class="form-group">
                                            <label for="sdId" class="col-md-1 control-label">编号:</label>
                                            <div class="col-md-3">
                                                <input name="sdId" id="Text1" type="text" class="form-control" ng-model="vm.SdId" />
                                            </div>
                                            <label for="sdPutInDate" class="col-md-1 control-label">日期:</label>
                                            <div class="col-md-3">
                                                <input name="sdPutInDate" type="date" class="form-control" ng-model="vm.SdPutInDate" />
                                            </div>
                                            <label for="sdCustomer" class="col-md-1 control-label">顾客:</label>
                                            <div class="col-md-3">
                                                <input name="functionDescription" id="Text2" type="text" class="form-control" ng-model="vm.SdCustomer" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="triggerDevUnit" class="col-md-1 control-label">单位:</label>
                                            <div class="col-md-3">
                                                <input name="triggerDevUnit" id="Text3" type="text" class="form-control" ng-model="vm.TriggerDevUnit" />
                                            </div>
                                            <label for="fillInPreparer"   class="col-md-2 control-label">填表人:</label>
                                            <div class="col-md-2">
                                                <input name="fillInPreparer" id="Text4" type="text" rows="2" class="form-control" ng-model="vm.FillInPreparer" />
                                            </div>
                                            <label for="fillInCheckPreparer" class="col-md-2 control-label">确认人:</label>
                                            <div class="col-md-2">
                                                <input name="fillInCheckPreparer" type="text" class="form-control" ng-model="vm.FillInCheckPreparer" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="installationlocation" class="col-md-1 control-label">规范说明:</label>
                                            <div class="col-md-5">
                                                <input name="installationlocation" type="text" class="form-control" ng-model="vm. ProductName" />
                                            </div>
                                            <label for="functionDescription" class="col-md-1 control-label">规范附件:</label>
                                            <div class="col-md-5">
                                                <textarea name="functionDescription" type="text" class="form-control" ng-model="vm.ProductNameDescriptionAttachmentPath" />
                                            </div>

                                        </div>
                                        <div class="form-group">
                                            <label for="installationlocation" class="col-md-1 control-label">预产量:</label>
                                            <div class="col-md-3">
                                                <input name="installationlocation" type="text"  class="form-control" ng-model="vm.ExpectedProductionNumber" />
                                            </div>
                                            <label for="functionDescription" type="text" class="col-md-1 control-label">样品数量:</label>
                                            <div class="col-md-3">
                                                <input name="functionDescription"  type="text" class="form-control" ng-model="vm.SampleDemandQuantity" />
                                            </div>
                                            <label for="functionDescription" class="col-md-1 control-label">需求日期:</label>
                                            <div class="col-md-3">
                                                <input name="functionDescription"  type="date" class="form-control" ng-model="vm.DemandDate" />
                                            </div>

                                        </div>
                                    </fieldset>

                                    <fieldset>
                                        <legend class="alert alert-info">研发说明</legend>
                                        <div class="form-group">
                                            <label for="installationlocation" class="col-md-1 control-label">接单编号:</label>
                                            <div class="col-md-3">
                                                <input name="installationlocation" class="form-control" ng-model="vm.RdId" />
                                            </div>
                                            <label for="functionDescription" class="col-md-1 control-label">接受日期:</label>
                                            <div class="col-md-3">
                                                <input name="functionDescription" type="date" class="form-control" ng-model="vm.AcceptRdIdData" />
                                            </div>
                                            <label for="functionDescription" class="col-md-1 control-label">接受签名:</label>
                                            <div class="col-md-3">
                                                <input name="functionDescription" rows="1" class="form-control" ng-model="vm.AcceptSdPreparer" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="installationlocation" class="col-md-1 control-label">是否新品:</label>
                                            <div class="col-md-3">
                                                <input name="installationlocation" class="form-control" ng-model="vm.IsHaveTheClassProduct" />
                                            </div>
                                            <label for="installationlocation" class="col-md-2 control-label">开发难度:</label>
                                            <div class="col-md-2">
                                                <select ng-model="vm.DevelopmentDifficultyLevel" ng-options="item.name  for item in vmManager.difficultyLevels"></select>
                                            </div>
                                            <label for="functionDescription" class="col-md-1 control-label">设备情况:</label>
                                            <div class="col-md-3">
                                                <textarea name="functionDescription" rows="1" class="form-control" ng-model="vm.ProductSpecAndEquipmentIsAllReady" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="functionDescription" class="col-md-4 control-label">设备/新产品的补充说明:</label>
                                            <div class="col-md-8">
                                                <textarea name="functionDescription" rows="1" class="form-control" ng-model="vm.ProductSpecAndEquipmentAllReadyDescription" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="functionDescription" class="col-md-1 control-label">是否量产:</label>
                                            <div class="col-md-3">
                                                <input name="functionDescription" class="form-control" ng-model="vm.IsNeedTryProduction" />
                                            </div>
                                            <label for="installationlocation" class="col-md-1 control-label">量产数量:</label>
                                            <div class="col-md-3">
                                                <input name="installationlocation" class="form-control" ng-model="vm.NeedTryProductionNumber" />
                                            </div>
                                            <label for="functionDescription" class="col-md-1 control-label">材料成本:</label>
                                            <div class="col-md-3">
                                                <textarea name="functionDescription" rows="1" class="form-control" ng-model="vm.TheCostEstimate" />
                                            </div>
                                        </div>
                                        <div class="form-group">

                                            <label for="functionDescription" class="col-md-2 control-label">负责人:</label>
                                            <div class="col-md-2">
                                                <input name="functionDescription" class="form-control" ng-model="vm.CertifiedScrumProductOwner" />
                                            </div>
                                            <label for="functionDescription" class="col-md-2 control-label">指派人:</label>
                                            <div class="col-md-2">
                                                <input name="functionDescription" class="form-control" ng-model="vm.DesignatedPerson" />
                                            </div>
                                            <label for="installationlocation" class="col-md-2 control-label">完成天数:</label>
                                            <div class="col-md-2">
                                                <input name="installationlocation" class="form-control" ng-model="vm.ScheduledCompletionDays" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="functionDescription" class="col-md-2 control-label">补充说明:</label>
                                            <div class="col-md-9">
                                                <textarea name="functionDescription"  rows="2" class="form-control" ng-model="vm.OtherSupplementDescription" />
                                            </div>
                                        </div>
                                    </fieldset>

                                    <hr />
                                    <div class="alert alert-info">
                                        <div class="row">
                                            <div class="col-md-2">
                                            </div>
                                            <div class="col-md-10">
                                                <button class="btn btn-primary" ng-click="operate.saveAll(formdevelopment.$valid)"><i class="fa fa-save"></i>保存</button>
                                                <button class="btn btn-default" ng-click="operate.refresh()"><i class="fa fa-undo"></i>取消</button>
                                                <span yl-operatemsg-board></span>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <ul class="list-group">
                                <li class="list-group-item active">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <h4 class="list-title">
                                                <i class="fa fa-list"></i>
                                                <span>列表</span>
                                            </h4>
                                        </div>
                                        <div class="col-md-8">
                                            <div class="input-group">
                                                <input class="form-control" type="text" />
                                                <div class="input-group-addon">
                                                    <i class="fa fa-search"></i>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                                <li class="list-group-item" ng-click="vmManager.selectDevelopmentOrder(item)"
                                    data-title="设计编号:{{item.DevelopmentRmId}}" data-bs-tooltip
                                    ng-repeat="item in vmManager.DevelopmentRmIds track by $index">
                                    编号：
                                    <span class="label label-success" style="cursor:pointer">{{item.RmId}}</span>
                                    -----
                                    设计产品名称：
                                    <span>
                                        <i class="fa fa-envelope-o"></i>
                                        {{item.ProductName}}
                                    </span>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
        </div>
        <div class="panel-footer" /> 
    </div>
    
</div>