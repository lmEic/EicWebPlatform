                        <div class="col-md-10">
                            <form class="form-inline" novalidate>
                                <div class="form-group">
                                    <div  yl-date-range date-from="vmManager.dateFrom" date-to="vmManager.dateTo"></div>
                                </div>
                                <div class="form-group">
                                    <label>状态:</label>
                                    <button class="btn btn-success " ng-model="vmManager.checkStatus"
                                            data-html="1" data-multiple="0" data-animation="am-flip-x" data-placeholder="审核状态"
                                            data-bs-options="item.value as item.label for item in vmManager.checkStatues track by $index" data-bs-select>
                                        <span class="caret"></span>
                                    </button>
                                </div>
                                <div class="form-group">
                                    <button role="button" class="btn btn-primary tipBox" ng-click="vmManager.queryIqcInspectionCnfigInfo(0)"><i class="fa fa-search"></i>查询</button>
                                </div>
                                <div class="form-group">
                                    <input class="form-control" type="text" placeholder="输入品名快速定位" ng-model="filterIqcOrder"
                                           bs-options="item.OrderId as item.OrderId for item in vmManager.erpOrderInfoDatasSet" limit="10" bs-typeahead />
                                </div>
                                <span yl-operatemsg-board></span>
                            </form>
                        </div>
                    </div>
                    @*查询主表*@
                    <div class="row">
                        <div class="col-md-12" id="querIqcMainWindow">
                            <div class="table-responsive" style="min-height:500px;" cg-busy="{promise:searchPromise}">
                                <table class="table table-striped table-hover table-bordered">
                                    <thead>
                                        <tr class="info">
                                            <th>品号</th>
                                            <th>品名</th>
                                            <th>规格</th>
                                            <th>图号</th>
                                            <th>检验项次</th>
                                            <th>部门</th>
                                            <th>版次</th>
                                            <th>操作(<i class="fa fa-pencil-square-o"></i>)</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="item in vmManager.iqcDataSets |filter:filterIqcOrder">
                                            <td>{{item.MaterialId}}</td>
                                            <td>{{item.MaterailName}}</td>
                                            <td>{{item.MaterialSpecify}} </td>
                                            <td>{{item.MaterialrawID}}</td>
                                            <td>
                                                <a href="javascript:;" ng-click="vmManager.getIqcDetailDatas(item)">{{item.InspectionItemCount}}</a>
                                            </td>
                                            <td>{{item.MaterialBelongDepartment}} </td>
                                            <td>{{item.ItemConfigVersion}}</td>
                                            <td>
                                                <label ng-show="item.CheckStatus=== '已审核'">审核人：{{item.OpPerson}}</label>
                                                <button type="button" class="btn btn-primary btn-xs" ng-click="vmManager.getIqcDetailDatas(item)" ng-show="item.CheckStatus=== '已审核'">已审核</button>
                                                <button type="button" class="btn btn-warning btn-xs" ng-click="vmManager.getIqcDetailDatas(item)" ng-show="item.CheckStatus== '未审核'">侍审核</button>
                                            </td>
                                        </tr>
                                    </tbody>
