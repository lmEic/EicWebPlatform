                                <span>
                                    <i class="fa fa-envelope-o"></i>
                                    {{item.ReceiverName}}
                                </span>
                            </li>
                        </ul>*@
                    </div>
                </div>
            </div>
        </div>
        <div class="panel-footer" />
    </div>
</div>