<div class="panel panel-primary" ng-controller="recetionExpressCtrl">
    <div class="panel-heading">
        <h6 class="panel-title">快递输录入表</h6>
    </div>
    <div class="DivInput">
        <div class="panel-body">
            <div bs-pane data-title="快递输录入登记" data-name="initTab">
                <div class="row">
                    <div class="col-md-8">
                        <div class="well">
                            <form role="form" name="formrecetionExpress" novalidate class="form-horizontal" onkeypress="if (event.keyCode === 13) return false;">
                                <fieldset>

                                    <div class="form-group">
                                        <label for="sdId" class="col-md-2 control-label">快递单号:</label>
                                        <div class="col-md-4">
                                            <input name="sdId" id="Text1" type="text" class="form-control" ng-model="vm.ExpressId" />
                                        </div>
                                        <label for="expectedProductionNumber" class="col-md-2 control-label">快递公司:</label>
                                        <div class="col-md-4">
                                            <input name="expectedProductionNumber" type="text" class="form-control" ng-model="vm.ExpressCompany" />
                                        </div>

                                    </div>

                                    <div class="form-group">
                                        <label for="triggerDevUnit" class="col-md-2 control-label">发货地址:</label>
                                        <div class="col-md-4">
                                            <input name="triggerDevUnit" id="Text3" type="text" class="form-control" ng-model="vm.SendGoodsCompanyAddress" />
                                        </div>
                                        <label for="fillInPreparer" class="col-md-2 control-label">发货件数:</label>
                                        <div class="col-md-4">
                                            <input name="fillInPreparer" id="Text4" type="number" rows="2" class="form-control" ng-model="vm.GoodsNumber" />
                                        </div>
                                    
                                    </div>
                                    <div class="form-group">
                                        <label for="fillInCheckPreparer" class="col-md-2 control-label">签收人:</label>
                                        <div class="col-md-4">
                                            <div class="input-group">
                                                <input name="safekeepUser" class="form-control" data-ensure-user-exist
                                                       ng-change="vmManager.getWorkerInfo(vm.ReceiverName,1)"
                                                       placeholder="输工号或姓名" ng-model="vm.ReceiverName" required />
                                                <div class="error-messages" ng-messages="formrecetionExpress.ReceiverName.$error" ng-if="formrecetionExpress.ReceiverNamer.$touched">
                                                    <small ng-message="checkingUserExist">该用户名不存在...</small>
                                                </div>
                                                <div class="error-messages" ng-messages="formEquipment.ReceiverName.$error" ng-show="operate.showValidation">
                                                    <small ng-message="required">签收人不能为空</small>
                                                </div>
                                                <div class="input-group-addon" ng-show="vmManager.isSingle">
                                                    <label class="label label-primary"> <i class="fa fa-user"></i> {{vm.ReceiverName}}</label>
                                                </div>
                                                <div class="input-group-btn" ng-hide="vmManager.isSingle">
                                                    <button type="button" class="btn btn-primary"
                                                            data-animation="am-flip-x"
                                                            bs-dropdown aria-haspopup="true" aria-expanded="false">
                                                        <span class="fa fa-chevron-circle-down"></span>
                                                    </button>
                                                    <div class="dropdown-menu" role="menu">
                                                        <ul class="list-group">
                                                            <li class="list-group-item" style="cursor:pointer" ng-click="vmManager.selectWorker(worker)"
                                                                ng-repeat="worker in vmManager.searchedWorkers track by $index" data-title="岗位：{{worker.Post}}" data-bs-tooltip>
                                                                <i class="fa fa-user"></i>
                                                                <span class="label label-primary">工号：{{worker.WorkerId}}</span>|
                                                                <span>姓名：{{worker.Name}}</span>|
                                                                <span>部门：{{worker.Department}}</span>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                       
                                        <label for="sdCustomer" class="col-md-2 control-label">收货人:</label>
                                        <div class="col-md-4">
                                            <div class="input-group">
                                                <input name="safekeepUser" class="form-control" data-ensure-user-exist
                                                       ng-change="vmManager.getWorkerInfo(vm.GetGoodsPerson,2)"
                                                       placeholder="输工号或姓名" ng-model="vm.GetGoodsPerson" required />
                                                <div class="error-messages" ng-messages="formrecetionExpress.GetGoodsPerson.$error" ng-if="formrecetionExpress.GetGoodsPerson.$touched">
                                                    <small ng-message="checkingUserExist">该用户名不存在...</small>
                                                </div>
                                                <div class="error-messages" ng-messages="formEquipment.GetGoodsPerson.$error" ng-show="operate.showValidation">
                                                    <small ng-message="required">签收人不能为空</small>
                                                </div>
                                                <div class="input-group-addon" ng-show="vmManager.isSingle2">
                                                    <label class="label label-primary"> <i class="fa fa-user"></i> {{vm.GetGoodsPerson}}</label>
                                                </div>
                                                <div class="input-group-btn" ng-hide="vmManager.isSingle2">
                                                    <button type="button" class="btn btn-primary"
                                                            data-animation="am-flip-x"
                                                            bs-dropdown aria-haspopup="true" aria-expanded="false">
                                                        <span class="fa fa-chevron-circle-down"></span>
                                                    </button>
                                                    <div class="dropdown-menu" role="menu">
                                                        <ul class="list-group">
                                                            <li class="list-group-item" style="cursor:pointer" ng-click="vmManager.selectWorker2(worker)"
                                                                ng-repeat="worker in vmManager.searchedWorkers track by $index" data-title="岗位：{{worker.Post}}" data-bs-tooltip>
                                                                <i class="fa fa-user"></i>
                                                                <span class="label label-primary">工号：{{worker.WorkerId}}</span>|
                                                                <span>姓名：{{worker.Name}}</span>|
                                                                <span>部门：{{worker.Department}}</span>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="sdPutInDate" class="col-md-2 control-label">收件日期:</label>
                                        <div class="col-md-4">
                                            <input name="sdPutInDate" type="date" class="form-control" ng-model="vm.ReceptionDate" />
                                        </div>
                                        <label for="expectedProductionNumber" class="col-md-2 control-label">取件人:</label>
                                        <div class="col-md-4">
                                            <div class="input-group">
                                                <input name="safekeepUser" class="form-control" data-ensure-user-exist
                                                       ng-change="vmManager.getWorkerInfo(vm.Consignee,3)"
                                                       placeholder="输工号或姓名" ng-model="vm.Consignee" required />
                                                <div class="error-messages" ng-messages="formrecetionExpress.Consignee.$error" ng-if="formrecetionExpress.Consignee.$touched">
                                                    <small ng-message="checkingUserExist">该用户名不存在...</small>
                                                </div>
                                                <div class="error-messages" ng-messages="formEquipment.Consignee.$error" ng-show="operate.showValidation">
                                                    <small ng-message="required">签收人不能为空</small>
                                                </div>
                                                <div class="input-group-addon" ng-show="vmManager.isSingle3">
                                                    <label class="label label-primary"> <i class="fa fa-user"></i> {{vm.Consignee}}</label>
                                                </div>
                                                <div class="input-group-btn" ng-hide="vmManager.isSingle3">
                                                    <button type="button" class="btn btn-primary"
                                                            data-animation="am-flip-x"
                                                            bs-dropdown aria-haspopup="true" aria-expanded="false">
                                                        <span class="fa fa-chevron-circle-down"></span>
                                                    </button>
                                                    <div class="dropdown-menu" role="menu">
                                                        <ul class="list-group">
                                                            <li class="list-group-item" style="cursor:pointer" ng-click="vmManager.selectWorker3(worker)"
                                                                ng-repeat="worker in vmManager.searchedWorkers track by $index" data-title="岗位：{{worker.Post}}" data-bs-tooltip>
                                                                <i class="fa fa-user"></i>
                                                                <span class="label label-primary">工号：{{worker.WorkerId}}</span>|
                                                                <span>姓名：{{worker.Name}}</span>|
                                                                <span>部门：{{worker.Department}}</span>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                                <hr />
                                <div class="alert alert-info">
                                    <div class="row">
                                        <div class="col-md-4">
                                        </div>
                                        <div class="col-md-6">
                                            <button class="btn btn-primary" ng-click="operate.saveAll(formrecetionExpress.$valid)"><i class="fa fa-save"></i>保存</button>
                                            <button class="btn btn-default" ng-click="operate.refresh()"><i class="fa fa-undo"></i>取消</button>
                                            <span yl-operatemsg-board></span>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="col-md-4">
                            <ul class="list-group">
                                <li class="list-group-item active">
                                    <form class="form-inline" novalidate>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <div class="input-group-btn">
                                                    <button type="button" ng-class="vmManager.isLocal===false?'btn btn-warning':'btn btn-success'"
                                                            ng-model="vmManager.isLocal" bs-checkbox>
                                                        <i ng-class="vmManager.isLocal===false?'fa fa-toggle-off':'fa fa-toggle-on'"></i>
                                                        <span ng-show="vmManager.isLocal===false">远程</span>
                                                        <span ng-show="vmManager.isLocal">本地</span>
                                                    </button>
                                                </div>
                                                <div class="input-group-addon">
                                                    <label>单号:</label>
                                                </div>
                                                <input type="text" class="form-control" ng-model="query.ExpressId" placeholder="输入单号" />
                                                <div class="input-group-btn" ng-show="!vmManager.isLocal">
                                                    <button class="btn btn-success" ng-click="vmManager.searchBy()"><i class="fa fa-search"></i>查询</button>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </li>
                                <li class="list-group-item" ng-click="vmManager.selectExpress(item)" cg-busy="{promise:searchPromise}"
                                    ng-repeat="item in vmManager.expresssDatas|filter:query.ExpressId track by $index">
                                    <h5>
                                        <span class="label label-default" style="cursor:pointer">{{item.WorkerId}}</span>
                                        <span class="label label-primary">{{item.WorkerName}}</span>
                                        <span class="label label-default">{{item.ProductName}}</span>
                                        <span class="label label-info">{{item.ProductSpecify}}</span>
                                        <span class="label label-default">{{item.PerCount}}件</span>
                                        <span class="label label-warning">{{item.DealwithType}}</span>
                                    </h5>
                                </li>
                            </ul>
                        @*<ul class="list-group">
                            <li class="list-group-item active">
                                <div class="row">
                                    <div class="col-md-4">
                                        <h4 class="list-title">
                                            <i class="fa fa-list"></i>
                                            <span>列表</span>
                                        </h4>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input class="form-control" type="text" />
                                            <div class="input-group-addon">
                                                <i class="fa fa-search"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </li>
                            <li class="list-group-item" ng-click="vmManager.selectExpress(item)"
                                data-title="设计编号:{{item.ExpressId}}" data-bs-tooltip
                                ng-repeat="item in vmManager.expresss track by $index">
